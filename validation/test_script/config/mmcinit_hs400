echo "[info]============================> eMMC init start......"
echo "[info]============================> cmd0 reset"
cmd0
echo "[info]============================> cmd1 eMMC initialization"
cmd1
echo "[info]============================> cmd2 card identification"
cmd2
echo "[info]============================> cmd3 get CID"
cmd3
echo "[info]============================> cmd9 get CSD"
cmd9 
echo "[info]============================> cmd7 select card"
cmd7
echo "switch to hs400 mode x8 bus and tuning"
mmcconfig -d
cmd8
echo "[info]============================> eMMC init end"
